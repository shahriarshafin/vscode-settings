		"description": "MIT License"
	},
	"backto": {
		"prefix": "Back To Top Button",
		"body": [
			"<p align=\"right\">",
			"    <b><a href=\"#\">â†¥ Back To Top</a></b>",
			"</p>"
		],
		"description": "Back To Top Button"
