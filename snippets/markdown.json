{
	// Place your snippets for cpp here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"italic text": {
		"prefix": "italic",
		"body": [
			"*italic*"
		],
		"description": "Text in italic form"
	},
	"Bold text": {
		"prefix": "bold",
		"body": [
			"**bold**"
		],
		"description": "Text in bold form"
	},
	"Strikethrough text": {
		"prefix": "delete",
		"body": [
			"~~Scratch this.~~"
		],
		"description": "Strikethrough this text"
	},
	"Unordered list": {
		"prefix": "ul",
		"body": [
			"",
			"- Unordered list 1"
		],
		"description": "Unordered sub list"
	},
	"Attach inline link": {
		"prefix": "link",
		"body": [
			"[link](https://)"
		],
		"description": "Attach inline link with text"
	},
	"Image": {
		"prefix": "insert image",
		"body": [
			"![alt text](https://github.com/adam-p/markdown-here/raw/master/src/common/images/icon48.png 'Logo Title Text')"
		],
		"description": "Insert image with alt"
	},
	"Inline code": {
		"prefix": "code",
		"body": [
			" `code here` "
		],
		"description": "Add Inline code"
	},
	"Add code block": {
		"prefix": "code-block",
		"body": [
			"```",
			"code block",
			"```"
		],
		"description": "Add code block"
	},
	"Insert table": {
		"prefix": "table",
		"body": [
			"| Tables        | Are           | Cool  |",
			"| ------------- |:-------------:| -----:|",
			"| col 3 is      | right-aligned |  ffdf |",
			"| col 2 is      | centered      |  ffff |",
			"| zebra stripes | are neat      |  dddd |"
		],
		"description": "Insert table"
	},
	"Block Quote": {
		"prefix": "quote",
		"body": [
			"",
			"> Blockquotes are very handy in email to emulate reply text.",
			"> This line is part of the same quote."
		],
		"description": "Block Quote"
	},
	"Horizontal rule": {
		"prefix": "hr",
		"body": [
			"---"
		],
		"description": "Horizontal rule"
	},
	"Youtube video": {
		"prefix": "video",
		"body": [
			"<a href=\"http://www.youtube.com/watch?feature=player_embedded&v=YOUTUBE_VIDEO_ID_HERE",
			"\" target=\"_blank\"><img src=\"http://img.youtube.com/vi/YOUTUBE_VIDEO_ID_HERE/0.jpg\" ",
			"alt=\"IMAGE ALT TEXT HERE\" width=\"240\" height=\"180\" border=\"10\" /></a>"
		],
		"description": "Youtube video"
	},
	"Line brake": {
		"prefix": "Line brake",
		"body": [
			"<br>",
			""
		],
		"description": "Line brake"
	},
